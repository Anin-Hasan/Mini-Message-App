<div>
  <% for(let i = 0; i < messages.length; i++){ %>
    <div>

      <h1>Message: <%= messages[i].text %></h1>
      <h2>User: <%= messages[i].user %></h1>
      <h3>Time: <%= messages[i].added %></h1>
        <a href="/message/<%= i %>"> See Details</a>
    </div>
  <% } %>
  <button> <a href="/new">Send a New Message.</a></button>
</div>
